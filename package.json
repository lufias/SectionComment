{
  "name": "sectioncomment",
  "displayName": "SectionComment",
  "description": "Generate clear ascii text comment to highlight section of code",
  "version": "0.1.0",
  "publisher": "LufiasDev",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:SectionComment-Create",
    "onCommand:SectionComment-CreateWithFooter",
    "onCommand:SectionComment-Bordered"
  ],
  "main": "./extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lufias/SectionComment.git",
    "directory": "master"
  },
  "contributes": {
    "commands": [
      {
        "command": "SectionComment-Create",
        "title": "SectionComment-Create"
      },
      {
        "command": "SectionComment-CreateWithFooter",
        "title": "SectionComment-Create with Footer"
      },
      {
        "command": "SectionComment-Bordered",
        "title": "SectionComment-Create with Border"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
